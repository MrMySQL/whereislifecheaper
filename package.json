{
  "name": "whereislifecheaper",
  "version": "1.0.0",
  "description": "Compare grocery basket prices across countries by scraping supermarket websites",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "NODE_ENV=production node dist/api/server.js",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "scraper:test": "ts-node scripts/test-scraper.ts",
    "scraper:run": "ts-node scripts/run-scraper.ts",
    "api": "ts-node src/api/server.ts",
    "rates:sync": "ts-node scripts/sync-exchange-rates.ts"
  },
  "keywords": [
    "price-comparison",
    "web-scraping",
    "grocery",
    "cost-of-living"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.10.9",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "playwright": "^1.40.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
